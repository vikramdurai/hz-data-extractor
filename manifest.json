{
  "name": "Hrit&Ziel Linkedin Data Extractor",
  "description": "In-browser data scraper tool for LinkedIn",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/talent/hire/*"],
      "js": ["app.js"]
    }
  ]
}